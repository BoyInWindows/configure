	# vim
	#ln -s ~/configure/vim/vimrc .vimrc
	
	# sudo 环境变量 别名方式
	alias sudop='sudo env PATH=$PATH'

## path

	# java jdk
	export JAVA_HOME=/opt/jdk1.8.0
	export PATH=$JAVA_HOME/bin:$PATH
	export CLASSPATH=.:$JAVA_HOME/lib/dt,jar:$JAVA_HOME/lib/tools.jar

